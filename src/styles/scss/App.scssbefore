.to-top {
  z-index: 3;
  opacity: 0.3;
  // background-color: aqua;
  background-color: #cf3472;
  width: 40px;
  height: 40px;
  position: fixed;
  bottom: 2%;
  right: 2%;
  border-radius: 5px;
  border: none;
  outline: none !important;
  display: flex;
  justify-content: center;
  align-items: center;
  transition: all 100ms ease-in-out;
  &:hover {
    opacity: 1;
  }
  .fa-chevron-up {
    color: #fff;
  }
}

.fadeMenu-appear,
.fadeMenu-enter {
  opacity: 0;
  z-index: 1;
  filter: brightness(100%);
}

.fadeMenu-appear-active,
.fadeMenu-enter.fadeMenu-enter-active {
  opacity: 1;
  transition: opacity 300ms linear 150ms;
  filter: brightness(100%);
}

.fadeMenu-exit {
  opacity: 0;
  filter: brightness(100%);
}

.fadeMenu-exit.fadeMenu-exit-active {
  filter: brightness(100%);
  opacity: 0;
  transition: opacity 150ms linear;
}


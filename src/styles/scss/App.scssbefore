.popup-menu {
  position: fixed;
  width: 100%;
  transform: skewX(-40deg) scale(0);
  transform-origin: left top;
  z-index: 98;
  background: rgba(0, 0, 0, 0.9);
  transition: all 150ms ease-in-out;
  &.is-open {
    transform: skewX(-40deg) scale(1);
  }
  display: flex;
}

.popup-nav {
  position: fixed;
  z-index: 99;
  left: -30%;
  padding: 0;
  opacity: 0;
  transition: all 200ms ease-in-out;
  &.animate {
    left: 4%;
    opacity: 1;
  }
  li {
    padding: 1rem 0;
    a {
      color: #f2f2f2;
      text-decoration: none;
      font-size: 1.3rem;
      font-weight: 600;
      &.active {
        color: #cf3472;
      }
    }
  }
}

.main-nav {
  @media all and (max-width: 767px) {
    .menu-button {
      position: absolute;
      top: 50%;
      right: 4%;
      transform: translateY(-50%);
      background: transparent;

      display: inline-block;
      cursor: pointer;

      .bar1,
      .bar2,
      .bar3 {
        width: 35px;
        height: 5px;
        background-color: #f2f2f2;
        margin: 6px 0;
        transition: 0.4s;
      }
      &.change .bar1 {
        transform: rotate(-45deg) translate(-9px, 6px);
      }

      &.change .bar2 {
        opacity: 0;
      }

      &.change .bar3 {
        transform: rotate(45deg) translate(-8px, -8px);
      }
    }
    .mobile-logo-link {
      position: absolute;
      top: 50%;
      left: 4%;
      transform: translateY(-50%);
      .mobile-logo {
      }
    }
  }
  z-index: 100;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  padding: 0;
  height: 60px;
  transition: all 200ms ease-in-out;
  &.dark-background {
    background: rgba(0, 0, 0, 0.9);
  }
  &.main-nav-fixed {
    position: fixed !important;
    background: rgba(0, 0, 0, 0.9);
    height: 60px;
    left: 0;
    top: 0;
    width: 100%;
  }
  ul {
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 0;
    margin-bottom: 0;
    padding: 0.5rem 0;
    li:first-child {
      margin: 0 1rem;
    }
    li {
      padding: 0 1rem;
      &:not(:nth-of-type(1)) a:hover {
        color: lighten(#cf3472, 10%);
      }
      a {
        text-decoration: none;

        color: #fff;
        // font-weight: bold;
        font-size: 18px;
        font-weight: 500;
        padding: 0.5rem;
        border-radius: 3px;
        transition: all 300ms ease-in-out;

        &.main-menu-active {
          // color: brown !important;
          color: #cf3472;
          &:hover {
            color: #cf3472;
          }
        }
      }
    }
  }
}


.contact-box {
  background: url('/images/background-location.jpg');
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  margin-top: 2rem !important;

  @media (max-width: 767px) and (orientation: portrait) {
    margin-top: 0 !important;
  }

  .businesshours-box {
    position: relative;
    @media (max-width: 767px) and (orientation: portrait) {
      margin-top: 1rem;
    }
    &::after {
      position: absolute;
      height: 80%;
      width: 3px;
      top: 10%;
      right: 0;
      background: #ccc;
      content: '';
      @media (max-width: 767px) {
        width: 0px;
      }
    }
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 2rem;
    @media (max-width: 767px) {
      padding: 0.5rem;
      justify-content: center;
    }
    div {
      width: 90%;
      @media (max-width: 1023px) {
        width: 98%;
      }
      @media (max-width: 767px) {
        width: 90%;
      }
    }
    .heading {
      display: flex;
      justify-content: center;
      h2 {
        .fa-clock {
          color: darken(cyan, 15%);
          margin-right: 1rem;
        }
        // color: #f2f2f2;
        @media (max-width: 1023px) {
          font-size: 34px;
        }
        @media all and (max-width: 767px) {
          font-size: 32px;
        }
        @media all and (max-width: 330px) {
          font-size: 26px;
        }
        // font-family: 'Raleway', sans-serif;
        font-family: 'Vesper Libre', serif;
        font-weight: 600;
        font-size: 38px;
        line-height: 1;
        .fa-calendar-alt {
          margin-right: 0.5rem;
        }
      }
    }
    hr {
      height: 2px;
      // margin-top: 5px;
      background: #d2d2d2;
      width: 50%;
    }
    .hours {
      // margin-top: 1.5rem;
      width: 100%;
      display: flex;
      flex-direction: column;
      align-items: center;
      .item {
        width: 100% !important;
        // color: #f2f2f2;
        @media all and (max-width: 767px) {

}

.contact-box {
  background: url('/images/background-location.jpg');
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  margin-top: 2rem !important;
  .businesshours-box {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    padding-left: 3rem;
    div {
      width: 90%;
    }
    .heading {
      display: flex;
      justify-content: center;
      h2 {
        .fa-clock {
          color: darken(cyan, 15%);
          margin-right: 1rem;
        }
        // color: #f2f2f2;

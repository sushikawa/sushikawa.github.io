.menu-nav {
  &.menu-nav-fixed {
    background: rgba(255, 232, 102, 0.9);
    top: 0;
    left: 0;
    width: 100%;
    position: fixed;
    z-index: 3;
    height: 40px;
    .menu-nav-inner {
      // padding-top: 23px;
      padding-top: 20px;
      @media all and (max-width: 767px) {
        padding-top: 22px;
      }
    }
  }
  font-family: 'Leckerli One', cursive;
  // background: #cf3472;
  // background: lighten(#ffd800, 20%);
  background: #ffe866;
  // background: #999;
  display: flex;
  flex-direction: row;
  justify-content: center;
  // justify-content: flex-start;
  align-items: center;
  // height: 56px;
  height: 44px;
  transition: all 300ms ease-in-out;
  padding: 0 5px;
  z-index: 1;
  overflow: hidden;

  @media all and (max-width: 767px) {
    // justify-content: start;
    padding: 0;

    .left-button,
    .right-button {
      // display: none;
    }
    .menu-nav-inner {
      padding-top: 0;

      ul {
        height: 64px;
        li {
          a {
            font-size: 90%;
          }
        }
      }
    }
  }
  .menu-nav-wrapper {
    display: flex;
    flex-wrap: nowrap;
    margin: 0;
    padding: 0;
    .button {
      width: 40px;
      cursor: pointer;
      outline: 0;
      border: 0;
      background: darken(#ffe866, 22%);
      transition: all 200ms ease-in-out;
      &:hover {
        background: darken(#ffe866, 30%);
      }
      .fa-chevron-left,
      .fa-chevron-right {
        color: #fff;
      }
    }
  }
  .menu-nav-inner {
    overflow: hidden;
    padding-top: 21px;
  }
  ul {
    height: 61px;
    padding: 4px;
    border-radius: 5px;
    display: flex;
    justify-content: flex-start;
    flex-wrap: nowrap;
    // overflow: hidden;
    overflow-y: hidden;
    overflow-x: auto;
    transition: all 500ms linear;

    @media all and (max-width: 767px) {
      justify-content: start;
    }
    margin: 0;

    li {
      padding: 4px;
      a {
        text-decoration: none;
        padding: 6px;

        border-radius: 3px;
        color: #333;
        transition: all 300ms ease-in-out;
        &:hover {
          color: lighten(#cf3472, 20%);
        }
        &.menu-nav-active {
          background: #cf3472;
          color: #f2f2f2;
        }
      }
    }
  }
  // }
}

